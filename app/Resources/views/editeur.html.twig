    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="description" content="Visualisations des compétences de la COMUE hesam">
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <meta name="robots" content="noindex, nofollow">


        <title>{% block title %}HESAM : cartographie des compétences de la COMUE HESAM{% endblock %}</title>
            <link rel="icon" href="{{ asset('favicon.ico') }}" />
            {% block stylesheets %}

                <!--Import Google Icon Font-->
            <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">


            <!--Import materialize.css-->
            <link rel="stylesheet"
                href="{{ asset('assets/vendor/materialize/dist/css/materialize.min.css') }}">
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            <link rel="stylesheet" href="{{ asset('css/editeur.css') }}">
        {% endblock %}

    </head>

    <body>
        <!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->


        {% block fos_user_content %}{% endblock fos_user_content %}

        {% include "_menu.html.twig" %}


        {% for msg in app.session.flashBag.get('success') %}
            <div class="container">
                <div class="card-panel">
                    <span class="teal-text">{{msg}}</span>
                </div>
            </div>
        {% endfor %}

        {% block body %}{% endblock %}

